{
  "name": "ml-ransac",
  "version": "0.0.0",
  "description": "Fit a model to noisy data by excluding outliers. This is an implementation of the RANSAC algorithm.",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "keywords": [],
  "author": "Oc√©ane Patiny",
  "license": "MIT",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mljs/ransac.git"
  },
  "bugs": {
    "url": "https://github.com/mljs/ransac/issues"
  },
  "homepage": "https://github.com/mljs/ransac#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.36.0",
    "eslint-config-cheminfo-typescript": "^11.3.1",
    "eslint-plugin-jest": "^27.2.1",
    "image-js": "0.0.0-next-4d1cb272ce",
    "jest": "^29.5.0",
    "jest-matcher-deep-close-to": "^3.0.2",
    "prettier": "^2.8.6",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "ml-array-median": "^1.1.6",
    "ml-levenberg-marquardt": "^4.1.0",
    "ml-matrix": "^6.10.4",
    "ml-random": "^0.5.0"
  }
